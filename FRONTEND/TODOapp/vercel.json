{
  "version": 2,
  "builds": [
    {
      "src": "FRONTEND/TODOapp/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "FRONTEND/TODOapp/dist/TODOapp",
        "buildCommand": "cd FRONTEND/TODOapp && npx ng build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}